<tr id="<%= curso.name %>">
  <td>
    <span><%= best_in_place curso, :name, tabindex: "1" %></span>
  </td>
  <td>
    <span><%= best_in_place curso, :professor_id, :class => "dropdown",
              :as => :select, :collection => @prof_selec %></span>
  </td>
  <td style="text-align: right">
    <%= link_to "X", curso, method: :delete, id: curso.id, data: { confirm: "borrar?", remote: true } %>
  </td>
</tr>
